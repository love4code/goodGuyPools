        footer {
          background-color: <%= (siteSettings && siteSettings.theme && siteSettings.theme.footer) ? siteSettings.theme.footer.backgroundColor : '#ffffff' %> !important;
          color: <%= (siteSettings && siteSettings.theme && siteSettings.theme.footer) ? siteSettings.theme.footer.textColor : '#6c757d' %> !important;
          border-top-color: <%= (siteSettings && siteSettings.theme && siteSettings.theme.footer) ? siteSettings.theme.footer.borderColor : '#dee2e6' %> !important;
        }
        footer a {
          color: <%= (siteSettings && siteSettings.theme && siteSettings.theme.footer) ? siteSettings.theme.footer.linkColor : '#3371A0' %> !important;
        }
        footer a:hover {
          color: <%= (siteSettings && siteSettings.theme && siteSettings.theme.footer) ? siteSettings.theme.footer.linkHoverColor : '#2a5d87' %> !important;
        }
      </style>
      <div class="container small d-flex flex-column flex-md-row justify-content-between align-items-center">
        <div>
          <strong><%= (siteSettings && siteSettings.companyName) || 'Goodfella Pools' %></strong>
          <div class="text-muted"><%= (siteSettings && siteSettings.address) || '' %></div>
          <div class="text-muted">
            <a href="tel:<%= (siteSettings && siteSettings.phone) || '' %>"><%= (siteSettings && siteSettings.phone) || '' %></a>
            Â·
            <a href="mailto:<%= (siteSettings && siteSettings.email) || '' %>"><%= (siteSettings && siteSettings.email) || '' %></a>
          </div>
        </div>
        <div class="mt-3 mt-md-0">
          <% if (siteSettings && siteSettings.social) { %>
            <% if (siteSettings.social.facebookUrl) { %>
              <a class="me-3" href="<%= siteSettings.social.facebookUrl %>" target="_blank" aria-label="Facebook">
                <i class="bi bi-facebook" style="font-size: 1.5rem;"></i>
              </a>
            <% } %>
            <% if (siteSettings.social.instagramUrl) { %>
              <a class="me-3" href="<%= siteSettings.social.instagramUrl %>" target="_blank" aria-label="Instagram">
                <i class="bi bi-instagram" style="font-size: 1.5rem;"></i>
              </a>
            <% } %>
            <% if (siteSettings.social.youtubeUrl) { %>
              <a class="me-3" href="<%= siteSettings.social.youtubeUrl %>" target="_blank" aria-label="YouTube">
                <i class="bi bi-youtube" style="font-size: 1.5rem;"></i>
              </a>
            <% } %>
            <% if (siteSettings.social.tiktokUrl) { %>
              <a class="me-3" href="<%= siteSettings.social.tiktokUrl %>" target="_blank" aria-label="TikTok">
                <i class="bi bi-tiktok" style="font-size: 1.5rem;"></i>
              </a>
            <% } %>
          <% } %>
          <a href="/admin/login">Login</a>
        </div>
        <div class="text-muted mt-3 mt-md-0">
          <div>&copy; <%= new Date().getFullYear() %> Goodfella Pools</div>
          <div class="mt-1">Affiliate of Aquarian Pool and Spa</div>
        </div>
      </div>
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>


